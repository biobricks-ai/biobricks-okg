PREFIX cosing: <http://example.org/cosing/>
PREFIX cosing.prop: <http://example.org/cosing/prop/>

PREFIX ex: <http://example.com/>

SELECT
  ?cosing_ref
  (COUNT(DISTINCT ?gene_id) AS ?gene_count)
  ?cosing_inci
  (GROUP_CONCAT(DISTINCT ?cas; SEPARATOR='|') AS ?cas_list)
  (GROUP_CONCAT(DISTINCT ?cosing_function; SEPARATOR=',') AS ?function_list)
{
  ?record ex:chemical_id ?cas ;
          ex:gene_id     ?gene_id .

  ?cas    a              cheminf:000446 .

  ?cosing_ref cosing.prop:casNo    ?cas ;
              cosing.prop:inciName ?cosing_inci ;
              cosing.prop:function ?cosing_function .
}
GROUP BY ?cosing_ref ?cosing_inci
ORDER BY DESC(?gene_count)
LIMIT 100
