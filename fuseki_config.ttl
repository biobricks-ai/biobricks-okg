@prefix :        <#> .
@prefix fuseki:  <http://jena.apache.org/fuseki#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix tdb:     <http://jena.hpl.hp.com/2008/tdb#> .
@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix hdt:      <http://www.rdfhdt.org/fuseki#> .

[] rdf:type fuseki:Server ;
   # Timeout - server-wide default: milliseconds.
   # Format 1: "1000" -- 1 second timeout
   # Format 2: "10000,60000" -- 10s timeout to first result, then 60s timeout for the rest of query.
   # See java doc for ARQ.queryTimeout
   # ja:context [ ja:cxtName "arq:queryTimeout" ;  ja:cxtValue "10000" ] ;

   # IMPORTANT: Import the HDT Assembler
   ja:loadClass "org.rdfhdt.hdtjena.HDTGraphAssembler" ;

   fuseki:services (
     <#service1>
   ) .

# HDT Classes
hdt:HDTGraph rdfs:subClassOf ja:Graph .

## ---------------------------------------------------------------
## Create a Read-Only Dataset composed by many RDF Graphs, each from an HDT File.

<#service1> rdf:type fuseki:Service ;
    fuseki:name                     "hdtservice" ;
    fuseki:serviceQuery             "query" ;
    fuseki:serviceReadGraphStore    "get" ;
    fuseki:dataset                   <#dataset> ;
    .

<#dataset> rdf:type ja:RDFDataset ;
    rdfs:label "Dataset" ;

    # HGNC {{{


    ja:namedGraph [
      ja:graphName <https://www.genenames.org/?file=data-source/internal-hgnc-kg/hgnc_complete_set.hdt>;
      ja:graph [
        a hdt:HDTGraph ;
        hdt:fileName """data-source/internal-hgnc-kg/hgnc_complete_set.hdt"""
      ]
    ] ;
    # }}}

    # ICE {{{


    ja:namedGraph [
      ja:graphName <https://ice.ntp.niehs.nih.gov/?file=brick/ice.hdt>;
      ja:graph [
        a hdt:HDTGraph ;
        hdt:fileName """brick/ice.hdt"""
      ]
    ] ;
    # }}}

    # MeSH {{{


    ja:namedGraph [
      ja:graphName <https://nlmpubs.nlm.nih.gov/projects/mesh/rdf/mesh.nt.gz?file=data-source/mesh-kg/mesh.hdt>;
      ja:graph [
        a hdt:HDTGraph ;
        hdt:fileName """data-source/mesh-kg/mesh.hdt"""
      ]
    ] ;
    # }}}

    # Tox21 {{{


    ja:namedGraph [
      ja:graphName <https://tripod.nih.gov/tox21/pubdata/?file=data-source/internal-tox21-kg/tox21.hdt>;
      ja:graph [
        a hdt:HDTGraph ;
        hdt:fileName """data-source/internal-tox21-kg/tox21.hdt"""
      ]
    ] ;
    # }}}

    # ToxCast {{{


    ja:namedGraph [
      ja:graphName <https://www.epa.gov/chemical-research/toxicity-forecasting?file=brick/toxcast.hdt>;
      ja:graph [
        a hdt:HDTGraph ;
        hdt:fileName """brick/toxcast.hdt"""
      ]
    ] ;
    # }}}

